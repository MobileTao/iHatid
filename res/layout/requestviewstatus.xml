<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout android:id="@+id/LinearLayout1"
  xmlns:android="http://schemas.android.com/apk/res/android"
  android:orientation="vertical"
  android:layout_width="match_parent"
  android:layout_height="match_parent">
    <TextView android:layout_width="wrap_content" android:layout_height="wrap_content" android:id="@+id/textView1" android:text="Destination Submitted" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="38dp"></TextView>
    
    <TableLayout android:layout_width="fill_parent" android:layout_height="wrap_content" android:id="@+id/tableLayout1" android:layout_below="@+id/textView3" android:layout_alignParentLeft="true" android:layout_alignParentRight="true">
        <TableRow  android:id="@+id/tableRow1" android:layout_width="fill_parent" android:layout_height="wrap_content" android:background="#AAAAAA">
        	<TextView android:layout_height="wrap_content" android:id="@+id/textView1" android:text="Wait (mins)" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="38dp" android:layout_weight="1" android:layout_width="0dp" android:gravity="center"></TextView>
        	<TextView android:layout_height="wrap_content" android:id="@+id/textView1" android:text="Operator" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="38dp" android:layout_weight="1" android:layout_width="0dp" android:gravity="center"></TextView>
        	<TextView android:layout_marginTop="38dp" android:text="Plate #" android:layout_weight="1" android:layout_height="wrap_content" android:id="@+id/textView1" android:layout_width="0dp" android:gravity="center"></TextView>
        	<TextView android:layout_marginTop="38dp" android:text="Rating" android:layout_weight="1" android:layout_height="wrap_content" android:id="@+id/textView1" android:layout_width="0dp" android:gravity="center"></TextView>
        </TableRow>
        
        <TableRow  android:id="@+id/tableRow1" android:layout_width="fill_parent" android:layout_height="wrap_content">
        	<TextView android:layout_height="wrap_content" android:id="@+id/textView1" android:text="15" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="38dp" android:layout_weight="1" android:layout_width="0dp" android:gravity="center"></TextView>
        	<TextView android:layout_height="wrap_content" android:id="@+id/textView1" android:text="MGE" android:layout_alignParentTop="true" android:layout_centerHorizontal="true" android:layout_marginTop="38dp" android:layout_weight="1" android:layout_width="0dp" android:gravity="center"></TextView>
        	<TextView android:layout_marginTop="38dp" android:text="ABC 123" android:layout_weight="1" android:layout_height="wrap_content" android:id="@+id/textView1" android:layout_width="0dp" android:gravity="center"></TextView>
        	<TextView android:layout_marginTop="38dp" android:text="*****" android:layout_weight="1" android:layout_height="wrap_content" android:id="@+id/textView1" android:layout_width="0dp" android:gravity="center"></TextView>
        </TableRow>
    </TableLayout>
    <TextView android:layout_height="wrap_content" android:text="@string/ack_successful" android:layout_width="wrap_content" android:id="@+id/textView3" android:layout_below="@+id/textView2" android:layout_centerHorizontal="true"></TextView>
    <TextView android:layout_height="wrap_content" android:text="&lt;1&gt; Ack (s)" android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content" android:id="@+id/textView2" android:layout_below="@+id/textView1" android:layout_centerHorizontal="true" android:layout_marginTop="21dp"></TextView>
    <Button android:layout_height="wrap_content" android:layout_width="wrap_content" android:layout_below="@+id/tableLayout1" android:layout_alignLeft="@+id/textView2" android:layout_marginTop="57dp" android:text="Cancel" android:id="@+id/buttonCancel"></Button>
    
</RelativeLayout>
